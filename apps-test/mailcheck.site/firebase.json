{
  "hosting": [
    {
      "target": "mailcheck-landing",
      "headers": [
        {
          "source": "**/*.@(js|xml|jpg|jpeg|gif|webp|png|avif|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=691200"
            }
          ]
        },
        {
          "source": "404.html",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=300"
            }
          ]
        }
      ],
      "public": "build",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "/sendMail",
          "function": "sendMail"
        },
        {
          "source": "/checkMail",
          "function": "checkMail"
        },
